	out = ""
	
	last = s[1]
	last_idx = 1
	for (idx, char) in s[1:length(s)-1]
		if char !== last
			out *= string(idx - last_idx) * last
		end
	end
	out *= string(length(s) - last_idx+1 ) * last
	out
